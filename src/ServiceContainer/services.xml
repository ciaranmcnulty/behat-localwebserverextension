<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="cjm.local_webserver.suite_listener" class="Cjm\Behat\LocalWebserverExtension\EventDispatcher\SuiteSubscriber">
            <argument type="service" id="cjm.local_webserver.webserver_controller.built_in" />
            <tag name="event_dispatcher.subscriber" />
        </service>

        <service id="cjm.local_webserver.webserver_controller.built_in" class="Cjm\Behat\LocalWebserverExtension\Webserver\BuiltInWebserverController" />

    </services>
</container>